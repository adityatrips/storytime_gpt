{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/DMSans-Black.ttf",
            "./src/assets/fonts/DMSans-Bold.ttf",
            "./src/assets/fonts/DMSans-ExtraBold.ttf",
            "./src/assets/fonts/DMSans-ExtraLight.ttf",
            "./src/assets/fonts/DMSans-Light.ttf",
            "./src/assets/fonts/DMSans-Medium.ttf",
            "./src/assets/fonts/DMSans-Regular.ttf",
            "./src/assets/fonts/DMSans-SemiBold.ttf",
            "./src/assets/fonts/DMSans-Thin.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "name": "StorytimeLLM",
    "slug": "storytime_gpt",
    "android": {
      "package": "com.adityatrips.storytime_gpt"
    },
    "ios": {
      "bundleIdentifier": "com.adityatripathi.storytimegpt"
    }
  }
}
